{
    "name": "package.json",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:commit": "vitest run --silent",
        "eslint-fix": "eslint --fix --ext .tsx,.ts src/ ",
        "lint": "eslint --ext .ts,.tsx src/",
        "prepare": "husky install .husky",
        "prettier-format": "npx prettier --write \"**/*.{ts,tsx}\""
    },
    "dependencies": {
        "@chakra-ui/react": "2.3.4",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@fontsource/oswald": "^4.5.11",
        "@fontsource/roboto": "^4.5.8",
        "@hookform/resolvers": "^2.9.10",
        "@tanstack/react-query": "^4.14.5",
        "axios": "^1.1.3",
        "camelcase-keys": "^8.0.2",
        "date-fns": "^2.29.3",
        "framer-motion": "^7.6.5",
        "husky": "^8.0.2",
        "jotai": "^1.9.1",
        "lint-staged": "^13.0.3",
        "lodash.samplesize": "^4.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.39.1",
        "react-router": "^6.4.3",
        "react-router-dom": "^6.4.3",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@tanstack/react-query-devtools": "^4.14.5",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.4.3",
        "@types/date-fns": "^2.6.0",
        "@types/jest": "^29.2.2",
        "@types/lodash.samplesize": "^4.2.7",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "@vitejs/plugin-react": "^2.2.0",
        "eslint": "^8.27.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jsdom": "^20.0.2",
        "msw": "^0.48.1",
        "typescript": "^4.6.4",
        "vite": "^3.2.3",
        "vite-tsconfig-paths": "^3.5.2",
        "vitest": "^0.25.1"
    },
    "husky": {
        "hooks": {
            "pre-push": "lint-staged",
            "pre-commit": "npm run lint && npm run check-format"
        }
    }
}
